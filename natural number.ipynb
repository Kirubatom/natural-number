    "def findSum(num):\n",
    "    sum=0\n",
    "    x=1\n",
    "    while x<= num:\n",
    "         sum+=x\n",
    "         x+=1\n",
    "    return sum\n",
    "num=int(input(\"enter a number:\"))\n",
    "print('THE SUM OF NATURAL NUMBER=',findSum(num))\n"
